{
    "name": "ghostzero/i18n",
    "description": "Laravel package to localize your project.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "René Preuß",
            "email": "rene@preuss.io"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "illuminate/console": "^6.0|^7.0|^8.1",
        "illuminate/support": "^6.0|^7.0|^8.1"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "GhostZero\\I18n\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GhostZero\\I18n\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
